Line @GetBorderByDirection( const uint8 direction, Map& map )
{
	MapHash @hash = @Global::World.GetHash( map );
	
	Hexagonal a();
	Hexagonal b();
	
	switch( direction )
	{
		case 0:
		case 1:
		case 2:
			a.X = hash.Min.X;
			a.Y = hash.Min.Y;
			b.X = hash.Min.X;
			b.Y = hash.Max.Y;
			break;
		case 3:
			a.X = hash.Min.X;
			a.Y = hash.Max.Y;
			b.X = hash.Max.X;
			b.Y = hash.Max.Y;
			break;
		case 4:
		case 5:
		case 6:
			a.X = hash.Max.X;
			a.Y = hash.Max.Y;
			b.X = hash.Max.X;
			b.Y = hash.Min.Y;
			break;
		case 7: 
			a.X = hash.Max.X;
			a.Y = hash.Min.Y;
			b.X = hash.Min.X;
			b.Y = hash.Min.Y;
			break;
			
		default: 
			break;
	}
	
	return @Line( a, b );
}

