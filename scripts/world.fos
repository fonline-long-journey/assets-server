	{
		if( queen.KindHierarchy == Kind::Hierarchy::Queen ) 
		{
			LifeQueen[queen.KindQIndex]--;
			const uint count = LifeQueen.length();
			for( uint i = 0; i < count; i++ )
				if( LifeQueen[i] != 0 )
					return;
			
			@content.CurrentKind = null;
		}
