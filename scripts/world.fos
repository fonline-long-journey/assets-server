	void Set( ::string& data )
	{
		::string@[] str = splitEx( data, "-" );
		if( str.length() > 0 )
		{
			if( str.length() == 2 )
			{
				::StrToInt( str[0], Min );
				::StrToInt( str[1], Max );
			}
			else if ( str.length() == 1 )
			{
				::StrToInt( str[0], Min );
				Max = Min;
			}
		}
		else Min = Max = 0;
	}

	void Set( int value )
	{
		Min = Max = value;
	}
	
