		{
            @map = GetMap( mapId );
            if( @map is null )
			{
				Log( "Map no valid - mapId");
                return 0;
			}
        } 
		else if( (locationId = cell.MapPack.MapPid) != 0 )
		{
			locationId = CreateLocation( cell.Position, locationId );
			if( locationId != 0 )
			{
				Location@ location = @GetLocation( locationId );
				if( @location !is null )
				{
					@map = location.GetMapByIndex(0);
